    "poem": ["",
        "Un premier jet",
        "D’idées troubles",
        "Qui s’embrouillent",
        "Dégoutte",
        "Mot à mot",
        "Et s’égoutte",
        "Phrase à phrase",
        "Et s’écoule",
        "Tout à coup",
        "D’un jet",
        "Une première vague",
        "D’idées claires",
        "Asperge",
        "La berge",
        "De la page",
        "De la plage",
        "Et me laisse",
        "Loin du large",
        "Dans le vague",
        "Une tempête d’idées folles",
        "Divague de la rive",
        "Et s’abat en un éclair",
        "De génie",
        "En une pluie qui se déchaîne",
        "En vent, en vers",
        "Et s’affole en un dernier jet",
        "Qui se jette dans une mer",
        "D’idées",
        "Enchaînées",
        "Au papier",
        "Par l’ancre qui pèse",
        "Ses mots",
        "Par l’encre qui sèche",
        "Même dans les tempêtes",
        "De vers d’eau"]
}